{
  "name": "patchfox",
  "version": "2019.8.2",
  "devDependencies": {
    "browserify": "^16.5.0",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "drag-and-drop-files": "0.0.1",
    "fs": "0.0.1-security",
    "is-my-json-valid": "^2.20.0",
    "lodash": "^4.17.15",
    "net": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "pull-stream": "^3.6.14",
    "query-string": "^6.8.2",
    "rimraf": "^2.6.3",
    "ssb-client": "^4.7.8",
    "svelte": "^3.7.1",
    "sveltify": "^3.0.0",
    "timeago-simple": "^1.2.4"
  },
  "scripts": {
    "clean": "rimraf dist",
    "copy:manifest": "copyfiles manifest.json dist",
    "copy:static": "copyfiles -E -u 1 \"static/**/*\" \"dist\"",
    "copy:docs": "copyfiles docs/**/* dist",
    "build:background": "browserify -d  src/background/background.js -o dist/background.js",
    "build:ui": "browserify -d  src/ui/main.js -o dist/bundle.js",
    "build": "run-s copy:* build:*",
    "dev": "run-p start:dev build",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev"
  },
  "browserify": {
    "transform": [
      [
        "sveltify",
        {
          "extensions": [
            ".svelte"
          ],
          "svelte": {
            "dev": true
          }
        }
      ]
    ]
  },
  "dependencies": {
    "private-box": "^0.3.0",
    "pull-box-stream": "^1.0.13",
    "pull-cat": "^1.1.11",
    "pull-catch": "^1.0.1",
    "pull-defer": "^0.2.3",
    "pull-file-reader": "^1.0.2",
    "pull-identify-filetype": "^1.1.0",
    "pull-many": "^1.0.8",
    "pull-next": "^1.0.1",
    "pull-next-query": "^1.0.0",
    "pull-paginate": "^1.0.0",
    "pull-paramap": "^1.2.2",
    "pull-reader": "^1.3.1",
    "pull-reconnect": "0.0.3",
    "pull-split": "^0.2.0",
    "pull-utf8-decoder": "^1.0.2",
    "ssb-avatar": "^0.2.0",
    "ssb-config": "^3.3.2",
    "ssb-feed": "^2.3.0",
    "ssb-gathering-schema": "^1.2.4",
    "ssb-git": "^0.5.0",
    "ssb-keys": "^7.1.5",
    "ssb-markdown": "^4.0.1",
    "ssb-marked": "^0.7.4",
    "ssb-mentions": "^0.5.0",
    "ssb-ref": "^2.13.9",
    "ssb-sort": "^1.1.3",
    "ssb-web-resolver": "^1.1.2",
    "stream-to-pull-stream": "^1.7.2"
  }
}
